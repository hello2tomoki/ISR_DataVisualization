  <DataSets>
    <DataSet Name="Derivatives_FX_Hedging_Program">
      <Query>
        <DataSourceName>DataSource_Sandbox</DataSourceName>
        <CommandText>
DECLARE @QY_Date DATE = '2018-06-29';


SELECT A.QY_Date,
       A.QY_Date_Label,
       A.Hedge_Order,
       A.Hedge_Type,
       A.Currrency,
       A.Currrency_Value,
	  Dense_Rank() Over (Partition by Type Order by Currrency_Sum)  as  Rnk,
      IIF(Dense_Rank() Over (Partition by Type Order by Currrency_Sum) % 2 =1, '#DCE6F1','White') as BG_Color,
       A.Type,
       A.Title,
	  A.Title_Order
FROM
(
    SELECT QY_Date,
           CASE
               WHEN Hedge_Type = 'Spot'
               THEN DATEADD(d, 0, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '01 Days'
               THEN DATEADD(d, 1, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '03 Days'
               THEN DATEADD(d, 2, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '04 Days'
               THEN DATEADD(d, 3, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '05 Days'
               THEN DATEADD(d, 4, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '06 Days'
               THEN DATEADD(d, 5, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '07 Days'
               THEN DATEADD(d, 6, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '10 Days'
               THEN DATEADD(d, 7, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '11 Days'
               THEN DATEADD(d, 8, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '12 Days'
               THEN DATEADD(d, 9, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '13 Days'
               THEN DATEADD(d, 10, CAST(QY_Date AS DATE))
               WHEN Hedge_Type = '14 Days'
               THEN DATEADD(d, 11, CAST(QY_Date AS DATE))
               ELSE ' '
           END AS QY_Date_Label,
           CASE
               WHEN Hedge_Type = 'Spot'
               THEN 1
               WHEN Hedge_Type = '01 Days'
               THEN 2
               WHEN Hedge_Type = '03 Days'
               THEN 3
               WHEN Hedge_Type = '04 Days'
               THEN 4
               WHEN Hedge_Type = '05 Days'
               THEN 5
               WHEN Hedge_Type = '06 Days'
               THEN 6
               WHEN Hedge_Type = '07 Days'
               THEN 7
               WHEN Hedge_Type = '10 Days'
               THEN 8
               WHEN Hedge_Type = '11 Days'
               THEN 9
               WHEN Hedge_Type = '12 Days'
               THEN 10
               WHEN Hedge_Type = '13 Days'
               THEN 11
               WHEN Hedge_Type = '14 Days'
               THEN 12
               WHEN Hedge_Type = '14-30 Days'
               THEN 13
               WHEN Hedge_Type = '30-90 Days'
               THEN 14
               WHEN Hedge_Type = '90-180 Days'
               THEN 15
               WHEN Hedge_Type = '180-360 Days'
               THEN 16
               WHEN Hedge_Type = 'More than 360 Days'
               THEN 17
           END AS Hedge_Order,
           F.Hedge_Type,
           F.Currrency,
           CAST(F.Currrency_Value AS FLOAT) Currrency_Value,
           SUM(CAST(F.Currrency_Value AS FLOAT)) OVER(PARTITION BY Type,
                                                                   F.Currrency) AS Currrency_Sum,
           F.[Type],
           CASE
               WHEN F.[Type] = 'Client'
               THEN 'Client FX Hedging Program'
               WHEN F.[Type] = 'bcimc'
               THEN 'bcimc FX Hedging Program'
           END AS Title,

		     CASE
               WHEN F.[Type] = 'Client'
               THEN 1
               WHEN F.[Type] = 'bcimc'
               THEN 2
           END AS Title_Order

    FROM [Stg_Rpt].[Derivatives_FX_Hedging_Program] F WITH (Nolock)

    WHERE CAST(QY_Date AS DATE) = @QY_Date
   
   ----------And  [Type] = 'client'
) AS A;



</CommandText>

  <DataSets>
    <DataSet Name="Details">
      <Query>
        <DataSourceName>DataSource_Sandbox</DataSourceName>
        <CommandText>DECLARE @SelectedDate AS DATE;
DECLARE @End_Month AS INT;
SET @SelectedDate = '2018-03-31';
--

    SELECT CAST(F.Market_Risk_Indicator AS FLOAT) OFR_Financial_Index,
		
         
		 Format(CAST(QY_Date AS DATE), 'MM/dd/yy') Date
			
    FROM Stg_Rpt.OFR_Financial_Stress_Index F  WITH (Nolock)
    WHERE CAST(QY_Date AS DATE) &lt;= @SelectedDate
  
 ORDER BY  Qy_Date</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
